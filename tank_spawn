    spawn_sub = random.randint(0,100)

    def tank_spawn(x):
        spawn_rate = 45
        if x < spawn_rate:
            return True
        else:
            return False
            tank_spawn(x)
                
    
    while tank_spawn(spawn_sub):
        v_tank = +0.05
        tank_x += v_tank*frame_rate
        if tank_x == 80:
            break
